{
  "openapi": "3.0.3",
  "info": {
    "title": "Sinap2i - Servizi Custom",
    "license": {
      "name": "2I Rete Gas"
    },
    "version": "1.0.1",
    "x-changelog": {
      "summary": "Changelog",
      "description": "This is the changelog for my API.",
      "versions": [
        {
          "version": "1.0.0",
          "date": "17-11-2023",
          "changes": [
            "First Release"
          ]
        },
        {
          "version": "1.0.1",
          "date": "04-12-2023",
          "changes": [
            "Ricerca/PDR - Response - distributore non pi√π obbligatorio"
          ]
        }
      ]
    }
  },
  "paths": {
    "/Ricerca/Appuntamento": {
      "post": {
        "requestBody": {
          "description": "Request Ricerca Appuntamento",
          "content": {
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/RicercaAppuntamento"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/RisultatiRicercaAppuntamento"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/badRequestResponse"
          },
          "401": {
            "$ref": "#/components/responses/unatuthorized"
          },
          "500": {
            "$ref": "#/components/responses/serverError"
          }
        }
      }
    },
    "/Ricerca/Pdr": {
      "post": {
        "requestBody": {
          "description": "Request Ricerca PDR",
          "content": {
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/RicercaPdr"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/RisultatiRicercaPdr"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/badRequestResponse"
          },
          "401": {
            "$ref": "#/components/responses/unatuthorized"
          },
          "500": {
            "$ref": "#/components/responses/serverError"
          }
        }
      }
    },
    "/Download/Preventivo": {
      "post": {
        "requestBody": {
          "description": "Request Download Preventivo",
          "content": {
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/IdentificativiRichiesta"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "headers": {
              "File-Name": {
                "description": "Nome del file binario condiviso",
                "schema": {
                  "type": "string"
                }
              },
              "File-Extension": {
                "description": "Estensione del file binario condiviso",
                "schema": {
                  "type": "string",
                  "enum": [
                    "pdf"
                  ]
                }
              },
              "File-Id": {
                "description": "Id univoco del file binario condiviso",
                "schema": {
                  "type": "integer"
                }
              },
              "File-Size": {
                "description": "Dimensione del file binario condiviso [byte]",
                "schema": {
                  "type": "integer"
                }
              }
            },
            "content": {
              "application/pdf": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/badRequestResponse"
          },
          "401": {
            "$ref": "#/components/responses/unatuthorized"
          },
          "500": {
            "$ref": "#/components/responses/serverError"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CodPratUtenteDistr": {
        "type": "string",
        "maxLength": 15
      },
      "CodPdr": {
        "type": "string",
        "pattern": "\\d{14}"
      },
      "Ora": {
        "type": "string",
        "pattern": "(0[0-9]|[1][0-9]|2[0123])[:](0[0-9]|[1-5][0-9])"
      },
      "Anno": {
        "type": "string",
        "pattern": "(19|20)\\d\\d"
      },
      "FasciaOraria": {
        "type": "string",
        "pattern": "(0[0-9]|[1][0-9]|2[0123])[:](0[0-9]|[1-5][0-9])[/](0[0-9]|[1][0-9]|2[0123])[:](0[0-9]|[1-5][0-9])"
      },
      "DataIta": {
        "type": "string",
        "pattern": "(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/(19|20)\\d\\d"
      },
      "ErroreRicerca": {
        "type": "object",
        "properties": {
          "esito": {
            "type": "string",
            "enum": [
              "KO",
              "OK"
            ]
          },
          "descrizione": {
            "type": "string"
          }
        }
      },
      "RicercaAppuntamento": {
        "type": "object",
        "required": [
          "cod_prat_distr"
        ],
        "properties": {
          "cod_prat_distr": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          },
          "cod_prat_utente": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          }
        }
      },
      "RisultatiRicercaAppuntamento": {
        "type": "object",
        "required": [
          "data_interrogazione",
          "cp_utente",
          "cod_prat_distr",
          "esito"
        ],
        "properties": {
          "data_interrogazione": {
            "$ref": "#/components/schemas/DataIta"
          },
          "cod_prat_utente": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          },
          "cod_prat_distr": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          },
          "esito": {
            "type": "integer",
            "enum": [
              0,
              1
            ],
            "description": "0=NO 1=SI"
          },
          "tipo_pratica": {
            "type": "string",
            "enum": [
              "D01",
              "PM1",
              "E01",
              "A40",
              "R01",
              "A01",
              "PN1",
              "R40",
              "PR1",
              "SM1",
              "SM2",
              "SW1"
            ]
          },
          "appuntamento": {
            "type": "string",
            "enum": [
              "SI",
              "NO"
            ],
            "description": "0=NO 1=SI"
          },
          "data_appuntamento": {
            "$ref": "#/components/schemas/DataIta"
          },
          "fascia_oraria_da": {
            "$ref": "#/components/schemas/Ora"
          },
          "fascia_oraria_a": {
            "$ref": "#/components/schemas/Ora"
          },
          "stato_appuntamento": {
            "type": "string",
            "enum": [
              "Da Confermare",
              "Non Confermato",
              "Confermato",
              "Annullato",
              "Evaso positivamente",
              "Evaso negativamente",
              "In attesa"
            ]
          },
          "evasione_appuntamento": {
            "type": "integer",
            "enum": [
              0,
              1
            ],
            "description": "0=Evasione Negativa 1=Evasione Positiva"
          },
          "motivo_evasione_negativa": {
            "type": "string"
          },
          "data_cut_off": {
            "$ref": "#/components/schemas/DataIta"
          },
          "ora_cut_off": {
            "$ref": "#/components/schemas/Ora"
          }
        }
      },
      "RicercaPdr": {
        "type": "object",
        "required": [
          "cod_pdr"
        ],
        "properties": {
          "cod_pdr": {
            "$ref": "#/components/schemas/CodPdr"
          }
        }
      },
      "RisultatiRicercaPdr": {
        "type": "object",
        "required": [
          "cod_pdr",
          "data_richiesta",
          "esito"
        ],
        "properties": {
          "cod_pdr": {
            "$ref": "#/components/schemas/CodPdr"
          },
          "data_richiesta": {
            "$ref": "#/components/schemas/DataIta"
          },
          "esito": {
            "type": "integer",
            "enum": [
              0,
              1,
              2,
              3
            ],
            "description": "0 - PDR NON ESISTENTE O NON DEL DISTRIBUTORE 1 - PDR IN TITOLARITA' 2 - PDR LIBERO  3 - PDR IN ALTRA TITOLARITA'"
          },
          "distributore": {
            "type": "string"
          },
          "indirizzo_pdr": {
            "$ref": "#/components/schemas/RicercaPdrIndirizzoPdr"
          },
          "dati_tecnici": {
            "$ref": "#/components/schemas/RicercaPdrDatiTecnici"
          },
          "contatore": {
            "$ref": "#/components/schemas/RicercaPdrContatore"
          },
          "correttore": {
            "$ref": "#/components/schemas/RicercaPdrCorrettore"
          },
          "dati_contrattuali": {
            "$ref": "#/components/schemas/RicercaPdrDatiContrattuali"
          }
        }
      },
      "RicercaPdrIndirizzoPdr": {
        "type": "object",
        "properties": {
          "toponimo": {
            "type": "string",
            "maxLength": 30
          },
          "indirizzo": {
            "type": "string"
          },
          "civico": {
            "type": "string",
            "maxLength": 10
          },
          "cap": {
            "type": "string",
            "pattern": "\\d{5}"
          },
          "comune_descrizione": {
            "type": "string",
            "maxLength": 100
          },
          "comune_istat": {
            "type": "string",
            "pattern": "\\d{6}"
          },
          "provincia": {
            "type": "string",
            "pattern": "[A-Za-z]{2}"
          },
          "nazione": {
            "type": "string"
          }
        }
      },
      "RicercaPdrDatiTecnici": {
        "type": "object",
        "properties": {
          "stato_pdr": {
            "type": "string",
            "enum": [
              "PAT",
              "ES",
              "DEM",
              "CED"
            ],
            "description": "PAT=in attesa prima attivazione ES=esistente DEM=demolito CED=ceduto"
          },
          "stato_allacciamento": {
            "type": "string",
            "enum": [
              "OK",
              "NGAS",
              "ITT",
              "VI",
              "CED",
              "DEM"
            ],
            "description": "OK=ok NGAS=non in gas ITT=interrotto VI=valvola interrotta CED=Ceduto DEM=Demolito"
          },
          "stato_impianto_interno": {
            "type": "string",
            "enum": [
              "OK",
              "NPR",
              "FUG",
              "MOD"
            ],
            "description": "OK=ok NPR=non predisposto FUG=fugante MOD=modificato"
          },
          "remi_pool": {
            "type": "string"
          },
          "tipo_gas": {
            "type": "string"
          },
          "potenzialita_massima_erogabile": {
            "type": "string"
          },
          "coeff_c": {
            "type": "string"
          }
        }
      },
      "RicercaPdrContatore": {
        "type": "object",
        "properties": {
          "matricola_contatore": {
            "type": "string",
            "maxLength": 20
          },
          "stato_contatore": {
            "type": "string",
            "enum": [
              "APT",
              "PIO",
              "RIM",
              "CED"
            ],
            "description": "APT=aperto PIO=piombato RIM=rimosso CED=Ceduto"
          },
          "chiuso_in_telegestione": {
            "type": "integer",
            "enum": [
              0,
              1
            ],
            "description": "0=NO 1=SI"
          },
          "cifre": {
            "type": "integer"
          },
          "data_posa": {
            "$ref": "#/components/schemas/DataIta"
          },
          "calibro": {
            "type": "string"
          },
          "tipo_contatore": {
            "type": "string",
            "enum": [
              "ELE",
              "STD"
            ],
            "description": "ELE=elettronico STD=standard"
          },
          "anno_fabbricazione_contatore": {
            "$ref": "#/components/schemas/Anno"
          },
          "accessibilita_alla_misura": {
            "type": "string"
          },
          "accessibilita_tecnica": {
            "type": "string"
          },
          "messa_in_servizio": {
            "$ref": "#/components/schemas/DataIta"
          },
          "teleletto": {
            "$ref": "#/components/schemas/DataIta"
          },
          "telegestito": {
            "$ref": "#/components/schemas/DataIta"
          }
        }
      },
      "RicercaPdrCorrettore": {
        "type": "object",
        "properties": {
          "matricola_correttore": {
            "type": "string",
            "maxLength": 20
          },
          "stato_correttore": {
            "type": "string",
            "enum": [
              "APT",
              "RIM",
              "SOS",
              "CHI"
            ],
            "description": "APT=aperto RIM=rimosso SOS=Sospeso CHI=Chiuso"
          },
          "cifre": {
            "type": "integer"
          },
          "data_posa": {
            "$ref": "#/components/schemas/DataIta"
          },
          "anno_fabbricazione_convertitore": {
            "$ref": "#/components/schemas/Anno"
          }
        }
      },
      "RicercaPdrDatiContrattuali": {
        "type": "object",
        "properties": {
          "cliente_finale": {
            "$ref": "#/components/schemas/RicercaPdrClienteFinale"
          },
          "beneficiario": {
            "$ref": "#/components/schemas/RicercaPdrBeneficiario"
          },
          "gestore_calore": {
            "$ref": "#/components/schemas/RicercaPdrGestoreCalore"
          },
          "tipo_pdr": {
            "type": "integer",
            "enum": [
              0,
              1,
              2,
              3
            ],
            "description": "0=Domestico 1=Condominio 2=Altri usi 3=Uso pubblico"
          },
          "consumo_annuo_pdr": {
            "type": "string"
          },
          "profilo_prelievo": {
            "type": "string",
            "enum": [
              "C1A1",
              "C1B1",
              "C1C1",
              "C1D1",
              "C1E1",
              "C1F1",
              "C2X1",
              "C3A1",
              "C3B1",
              "C3C1",
              "C3D1",
              "C3E1",
              "C3F1",
              "C4X1",
              "C5A1",
              "C5B1",
              "C5C1",
              "C5D1",
              "C5E1",
              "C5F1",
              "T1X1",
              "T1X2",
              "T1X3",
              "T2A1",
              "T2B1",
              "T2C1",
              "T2D1",
              "T2E1",
              "T2F1",
              "T2A2",
              "T2B2",
              "T2C2",
              "T2D2",
              "T2E2",
              "T2F2",
              "T2A3",
              "T2B3",
              "T2C3",
              "T2D3",
              "T2E3",
              "T2F3"
            ]
          },
          "trattamento_fini_settlement": {
            "type": "string"
          },
          "frequenza_lettura": {
            "type": "string"
          },
          "flag_disalimentabilita": {
            "type": "string",
            "enum": [
              "SI",
              "NO"
            ],
            "description": "0=NO 1=SI"
          },
          "flag_morosita": {
            "type": "string",
            "enum": [
              "SI",
              "NO"
            ],
            "description": "0=NO 1=SI"
          },
          "mercato": {
            "type": "string"
          }
        }
      },
      "RicercaPdrClienteFinale": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "maxLength": 50
          },
          "cognome": {
            "type": "string",
            "maxLength": 50
          },
          "ragione_sociale": {
            "type": "string",
            "maxLength": 100
          },
          "cf": {
            "type": "string",
            "maxLength": 16
          },
          "piva": {
            "type": "string",
            "pattern": "\\d{11}"
          }
        }
      },
      "RicercaPdrBeneficiario": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "maxLength": 50
          },
          "cognome": {
            "type": "string",
            "maxLength": 50
          },
          "ragione_sociale": {
            "type": "string",
            "maxLength": 100
          },
          "cf": {
            "type": "string",
            "maxLength": 16
          },
          "piva": {
            "type": "string",
            "pattern": "\\d{11}"
          }
        }
      },
      "RicercaPdrGestoreCalore": {
        "type": "object",
        "properties": {
          "nome": {
            "type": "string",
            "maxLength": 50
          },
          "cognome": {
            "type": "string",
            "maxLength": 50
          },
          "ragione_sociale": {
            "type": "string",
            "maxLength": 100
          }
        }
      },
      "IdentificativiRichiesta": {
        "type": "object",
        "required": [
          "piva_utente",
          "piva_distr",
          "cod_prat_utente",
          "cod_prat_distr"
        ],
        "properties": {
          "piva_utente": {
            "type": "string",
            "pattern": "\\d{11}"
          },
          "piva_distr": {
            "type": "string",
            "pattern": "\\d{11}"
          },
          "cod_prat_utente": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          },
          "cod_prat_distr": {
            "$ref": "#/components/schemas/CodPratUtenteDistr"
          }
        }
      }
    },
    "responses": {
      "badRequestResponse": {
        "description": "Risposta di Bad Request",
        "content": {
          "application/xml": {
            "schema": {
              "$ref": "#/components/schemas/ErroreRicerca"
            }
          }
        }
      },
      "unatuthorized": {
        "description": "Risposta di Unauthorized",
        "content": {
          "application/xml": {
            "schema": {
              "$ref": "#/components/schemas/ErroreRicerca"
            }
          }
        }
      },
      "serverError": {
        "description": "Risposta di Server Error",
        "content": {
          "application/xml": {
            "schema": {
              "$ref": "#/components/schemas/ErroreRicerca"
            }
          }
        }
      }
    }
  }
}